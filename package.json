{
  "name": "connect-bi-reporter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run watch & npm run preview",
    "watch": "vite build --watch --mode development",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage.enabled",
    "test:watch": "vitest watch",
    "lint": "eslint ./ui --ext .vue,.js",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ./ui",
    "format:check": "prettier --check ./ui",
    "lint-format": "npm run lint:fix && npm run format",
    "precommit": "npm run test && npm run lint-format && npm run build"
  },
  "dependencies": {
    "@cloudblueconnect/connect-ui-toolkit": "^31.0.0",
    "filesize": "^10.1.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-vite": "^2.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "vite": "^5.1.5",
    "vitest": "^1.3.1"
  }
}
