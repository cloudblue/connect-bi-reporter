{
  "name": "connect-bi-reporter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run watch & npm run preview",
    "watch": "vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage.enabled",
    "test:watch": "vitest watch",
    "lint": "eslint ./ui --ext .vue,.js",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ./ui",
    "format:check": "prettier --check ./ui",
    "lint-format": "npm run lint:fix; npm run format"
  },
  "dependencies": {
    "@cloudblueconnect/connect-ui-toolkit": "^30.3.0",
    "vue": "^3.3.10",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitest/coverage-v8": "^1.3.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.0.3",
    "vite": "^5.0.5",
    "vitest": "^1.0.1"
  }
}
